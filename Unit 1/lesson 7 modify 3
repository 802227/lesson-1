import math

x = input("Please input a whole number: ")
x = int(x)
if x > 20
  print ("sorry that number is not between 1 and 20")
else:
    if x > 1
      print ("sorry that number is not between 1 and 20")
else:
y = input("Please input another nonzero whole number")
y = int(y)
y != 0
if y == 0
   print ("please don enter zero")
  else:
   print("Now deciding if", y, "is a factor of", x, "...")
  rem = x % y
  if rem == 0:
    print("Yes!", y, "is a factor of", x)
  else: print ("sorry" , x, "is not a factor of" , y)
